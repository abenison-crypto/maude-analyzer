<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.maude-analyzer.weekly-refresh</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/alexbenison/maude-analyzer/scripts/cron_weekly_refresh.sh</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/alexbenison/maude-analyzer</string>

    <key>StartCalendarInterval</key>
